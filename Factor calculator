package com.company;
import java.util.*;

public class twofactorial {
    public static void main(String[] args) {
        int number;
        System.out.println("Enter the number: ");
        Scanner s = new Scanner(System.in);
        number = s.nextInt();
        s.close();
        long fact = 1;
        int i = 1;
        while(i<=number)
        {
            fact = fact * i;
            i++;
        }
        System.out.println("Factorial of "+number+" is: "+fact);
    }
    }

