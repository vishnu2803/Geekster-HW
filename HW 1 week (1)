
//Write pseudocode for printing 1 number from beginning and 1 number from end for the given range [1-100]--> [2,99]-->[3,97]
        
		example output:  1,100,2,99,3,98,4,97,5,96,6,95........ 

class Main{
    public static void main(String[] args) {
        int i=1;
        if(i<=100){
            for(int j=100;j>1;j--){
                System.out.print(i+","+j+",");
                i=i+1;
            }
        }  }   
         }
