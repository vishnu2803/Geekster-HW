package com.company;
import java.util.Scanner;

public class Patterns_ {
    public static void main(String[] args) {
        //  Left Triangle Star Pattern
        int i, j, row = 6;
        for (i=0; i<row; i++)
        {
            for (j=2*(row-i); j>=0; j--)
            {
                System.out.print(" ");
            }
            for (j=0; j<=i; j++ )
            {
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
