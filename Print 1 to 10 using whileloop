package com.company;
import java.util.*;

public class Print_1To10_usingWhile {
    public static void main(String[] args) {
        //Write a program to print 1 to 10 using while loop
        int i=1;
        System.out.println("output is = ");
        //loop from 1 to 10
        while (i<=10 ){
            System.out.println(i);
            i++;
             
        }
    }
}
