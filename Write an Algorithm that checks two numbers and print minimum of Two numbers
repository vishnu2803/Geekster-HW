package com.company;
import java.util.*;

public class TwoNumberPrint {
    //Write an Algorithm that checks two numbers and print minimum of Two numbers
    public static void main(String[] args) {
        int numberOne, numberTwo, Smallest;
        Scanner scan = new Scanner(System.in);

        System.out.println("Enter the first number: ");
        numberOne = scan.nextInt();
        System.out.println("Enter the Second number: ");
        numberTwo = scan.nextInt();

        if (numberOne < numberTwo)
            Smallest = numberOne;
        else
            Smallest = numberTwo;

        System.out.println( "\nSmallest = "+ Smallest);

    }
}


// write an algoritm of above
// step 1 = start
// step 2= input a & b 
// step 3= read a & b 
// step 4= check a & b less than 
// step 5= print "true" or "false"
// step 6= end
